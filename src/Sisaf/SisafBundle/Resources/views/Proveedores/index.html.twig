<h1>Proveedores list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nombre</th>
            <th>Rfc</th>
            <th>Pais</th>
            <th>Estatus</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('proveedores_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.Nombre }}</td>
            <td>{{ entity.RFC }}</td>
            <td>{{ entity.Pais }}</td>
            <td>{{ entity.Estatus }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('proveedores_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('proveedores_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('proveedores_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
