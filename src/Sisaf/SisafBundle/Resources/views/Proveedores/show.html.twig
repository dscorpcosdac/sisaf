{% extends '::base.html.twig' %}
{% block body %}

<h1>Proveedores</h1>

<table class="record_properties table">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Nombre</th>
            <td>{{ entity.Nombre }}</td>
        </tr>
        <tr>
            <th>Rfc</th>
            <td>{{ entity.RFC }}</td>
        </tr>
        <tr>
            <th>Pais</th>
            <td>{{ entity.Pais }}</td>
        </tr>
        <tr>
            <th>Estatus</th>
            <td>{{ entity.Estatus }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('proveedores') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('proveedores_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('proveedores_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>

{% endblock %}