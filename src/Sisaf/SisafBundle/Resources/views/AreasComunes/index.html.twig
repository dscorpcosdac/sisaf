{% extends '::base.html.twig' %}
{% block body %}

<h1>AreasComunes list</h1>

<table class="records_list table table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nombre</th>
            <th>Persona</th>
            <th>Fecha</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('areascomunes_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.Nombre }}</td>
            <td>{{ entity.Persona }}</td>
            <td>{{ entity.Fecha }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('areascomunes_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('areascomunes_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

{% endblock %}