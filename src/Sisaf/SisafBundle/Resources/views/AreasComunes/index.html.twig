{% extends '::base.html.twig' %}
{% block body %}

<h1>Lista de reservaciones para Areas Comunes</h1>

<table class="records_list table table-hover panel panel-default">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nombre</th>
            <th>Persona</th>
            <th>Fecha de reservaci√≥n</th>
            <th>Fecha de Registro</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('areascomunes_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.Nombre }}</td>
            <td>{{ entity.Persona }}</td>
            <td>{{ entity.Fecha|date('d/m/Y') }}</td>
            <td>{{ entity.FechaRegistro|date('d/m/Y') }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('areascomunes_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('areascomunes_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

{% endblock %}