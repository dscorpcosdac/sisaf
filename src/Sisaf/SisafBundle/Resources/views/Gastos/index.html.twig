{% extends '::base.html.twig' %}
{% block body %}

<h1>Gastos</h1>

<table class="records_list table table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>Tipo</th>
            <th>Concepto</th>
            <th>Descripcion</th>
            <th>Monto</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('gastos_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.Tipo }}</td>
            <td>{{ entity.Concepto }}</td>
            <td>{{ entity.Descripcion }}</td>
            <td>{{ entity.Monto }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('gastos_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('gastos_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

{% endblock %}