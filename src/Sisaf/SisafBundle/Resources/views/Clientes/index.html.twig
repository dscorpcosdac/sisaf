<h1>Clientes list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nombres</th>
            <th>Apellidos</th>
            <th>Tipo</th>
            <th>Genero</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('clientes_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.Nombres }}</td>
            <td>{{ entity.Apellidos }}</td>
            <td>{{ entity.Tipo }}</td>
            <td>{{ entity.Genero }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('clientes_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('clientes_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('clientes_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
