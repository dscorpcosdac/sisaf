<h1>Clientes</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Nombres</th>
            <td>{{ entity.Nombres }}</td>
        </tr>
        <tr>
            <th>Apellidos</th>
            <td>{{ entity.Apellidos }}</td>
        </tr>
        <tr>
            <th>Tipo</th>
            <td>{{ entity.Tipo }}</td>
        </tr>
        <tr>
            <th>Genero</th>
            <td>{{ entity.Genero }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('clientes') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('clientes_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('clientes_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
