{% extends '::base.html.twig' %}
{% block body %}

<h1>QuejSug</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Titulo</th>
            <td>{{ entity.Titulo }}</td>
        </tr>
        <tr>
            <th>Descripcion</th>
            <td>{{ entity.Descripcion }}</td>
        </tr>
        <tr>
            <th>Fecha</th>
            <td>{{ entity.Fecha|date('d-m-Y') }}</td>
        </tr>
        <tr>
            <th>Hora</th>
            <td>{{ entity.Hora|date('H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('quejsug') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('quejsug_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('quejsug_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>

{% endblock %}