<?php

namespace Sisaf\SisafBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * ingresosCuotas
 */
class ingresosCuotas
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var integer
     */
    private $ingreso;

    /**
     * @var integer
     */
    private $cuota;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set ingreso
     *
     * @param integer $ingreso
     * @return ingresosCuotas
     */
    public function setIngreso($ingreso)
    {
        $this->ingreso = $ingreso;

        return $this;
    }

    /**
     * Get ingreso
     *
     * @return integer 
     */
    public function getIngreso()
    {
        return $this->ingreso;
    }

    /**
     * Set cuota
     *
     * @param integer $cuota
     * @return ingresosCuotas
     */
    public function setCuota($cuota)
    {
        $this->cuota = $cuota;

        return $this;
    }

    /**
     * Get cuota
     *
     * @return integer 
     */
    public function getCuota()
    {
        return $this->cuota;
    }
}